[user]
  name = SET_ME_LOCALLY
  email = SET_ME_LOCALLY
[core]
  ignorecase   = false
[alias]
  graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  co    = checkout
  cam   = commit --amend
  ap    = add -p
  lp    = log -p
  ln    = log --name-only
  d     = diff
  dc    = diff --cached
  u     = pull --ff-only
[init]
  templatedir = ~/dotfiles/git_template
[multiuser]
  confdir = ~/dotfiles/git_user_configs
