[core]
  ignorecase   = false
[alias]
  graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  co    = checkout
  cam   = commit --amend
  cm    = commit -m
  ap    = add -p
  l     = log
  lp    = log -p
  ln    = log --name-only
  d     = diff
  dc    = diff --cached
  u     = pull --ff-only
  s     = status
[init]
  templatedir = ~/dotfiles/git_template
[multiuser]
  confdir = ~/dotfiles/git_user_configs
[user]
  name = 'Sho Mizoe'
  email = 'sho.mizoe@gmail.com'
