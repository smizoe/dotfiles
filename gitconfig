[core]
  ignorecase   = false
	autocrlf = false
[alias]
  graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  b     = branch
  bl    = branch --list
  co    = checkout
  cam   = commit --amend
  cm    = commit -m
  ap    = add -p
  l     = log
  lp    = log -p
  ln    = log --name-status
  d     = diff
  dc    = diff --cached
  dn    = diff --name-status
  dcn   = diff --cached --name-status
  u     = pull --rebase --autostash
  poh   = push origin HEAD
  ras   = rebase --autostash
  s     = status
  sl    = stash list
  wt    = worktree
[init]
  templatedir = ~/dotfiles/git_template
[multiuser]
  confdir = ~/dotfiles/git_user_configs
[user]
  name = 'Sho Mizoe'
  email = 'sho.mizoe@gmail.com'
